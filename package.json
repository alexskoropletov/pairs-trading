{
  "name": "stock-analyzer",
  "version": "1.0.0",
  "description": "Simple stock price fetcher",
  "main": "dist/fetch_prices.js",
  "scripts": {
    "build": "tsc",
    "start": "start.sh",
    "prices": "npm run build && node dist/fetch_prices.js",
    "portfolio": "npm run build && node dist/markowitz_portfolio.js",
    "plot-returns": "npm run build && node dist/plot_returns.js",
    "pairs-trading": "npm run build && node dist/pairs_trading.js",
    "get-stocks": "npm run build && node dist/get_s_n_p.js",
    "dev": "ts-node src/fetch_prices.ts",
    "dev:portfolio": "ts-node src/markowitz_portfolio.ts",
    "dev:plot": "ts-node src/plot_returns.ts",
    "dev:pairs": "ts-node src/pairs_trading.ts",
    "dev:stocks": "ts-node src/get_s_n_p.ts",
    "telegram": "npm run build && node dist/telegram_sender.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:unit:watch": "jest --testPathPattern=unit --watch",
    "test:e2e:watch": "jest --testPathPattern=e2e --watch"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.1",
    "fs-extra": "^11.1.1",
    "ml-matrix": "^6.10.4",
    "nodeplotlib": "^1.1.2",
    "readline": "^1.3.0",
    "telegram": "^2.22.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "stocks",
    "finance",
    "data",
    "portfolio"
  ],
  "author": "",
  "license": "MIT"
}
